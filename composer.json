{
  "name": "jkrrv/wedding-website",
  "description": "Julia and I are getting married.  This is the WordPress installation.",
  "minimum-stability": "stable",
  "license": "AGPL-3.0-or-later",
  "type": "project",
  "authors": [
    {
      "name": "James K",
      "email": "jkrrv@jkrrv.org"
    }
  ],
  "require": {
    "php": ">=7.4",
    "johnpbloch/wordpress": ">=5.4",
    "wpackagist-theme/catch-wedding":"^2.1"

  },
  "extra": {
    "installer-paths": {
      "wp/wp-content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "wp/wp-content/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    },
    "wordpress-install-dir": "wp"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "config": {
    "vendor-dir": "wp/wp-content/vendor"
  }
}